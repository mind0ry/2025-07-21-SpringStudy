package com.sist.main3;
import java.util.*;
public class ApplicationContext {
	private Map clsMap=new HashMap();
	public ApplicationContext() {
		clsMap.put("a", new A());
		clsMap.put("b", new A());
		clsMap.put("c", new A());
	}
	public Print getBean(String key) {
		return (Print)clsMap.get(key);
	}
}
